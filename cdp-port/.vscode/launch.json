    "version": "0.2.0",
    "configurations": [
        {
            "name": "cdp-port(Python) 시작",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/python/cdp-port.py",
            "args": ["--mode", "async"],
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "cdp-port(node.js) 시작",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/node/app01.js",
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "name": "mainBoostBeast(gdb) 시작",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/mainBoostBeast",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "gdb에 자동 서식 지정 사용",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "디스어셈블리 버전을 Intel(으)로 설정",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
